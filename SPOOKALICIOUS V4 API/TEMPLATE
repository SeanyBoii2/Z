    while true do
        task.wait(0.1)
        
        if _G.ESPEnabled then
            -- Your ESP update logic here
            -- Example: updateESP(), refreshPlayerList(), etc.
        end
    end
end)
--]]

-- Example: Character respawn handler
--[[
player.CharacterAdded:Connect(function(character)
    task.wait(1)
    
    -- Reapply settings after respawn
    if _G.SpeedEnabled then
        local humanoid = character:FindFirstChild("Humanoid")
        if humanoid then
            humanoid.WalkSpeed = _G.WalkSpeed or 16
        end
    end
end)
--]]

