    Toggled = true
}, function(value)
    _G.AntiAFK = value
end)

-- â–¼â–¼â–¼ WORKING CODE - Already active! â–¼â–¼â–¼
local VirtualUser = game:GetService("VirtualUser")
player.Idled:Connect(function()
    if _G.AntiAFK then
        VirtualUser:CaptureController()
        VirtualUser:ClickButton2(Vector2.new())
    end
end)

-- Info section
local InfoSection = MiscPage:CreateSection("Information")

InfoSection:CreateLabel("Script: " .. SCRIPT_NAME .. " " .. VERSION)
InfoSection:CreateLabel("Discord: " .. DISCORD)
InfoSection:CreateDivider()

InfoSection:CreateButton("Copy Discord", function()
    if setclipboard then
        setclipboard(DISCORD)
        Window:Notify("Discord copied!")
    else
        Window:Notify("Discord: " .. DISCORD)
    end
end)

InfoSection:CreateButton("Destroy GUI", function()
    Window:Notify("Goodbye!")
    task.wait(1)
    Window:Destroy()
end)


--â•”â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•—
--â•‘                     CONFIG PAGE (KEEP THIS!)                             â•‘
--â•‘                                                                          â•‘
--â•‘  DON'T DELETE - This lets users save/load their settings                â•‘
--â•šâ•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•

local ConfigPage = Window:CreatePage("Config")
local SaveLoadSection = ConfigPage:CreateSection("Profiles")

SaveLoadSection:CreateLabel("Save and load your settings")
SaveLoadSection:CreateDivider()

-- Save config
SaveLoadSection:CreateButton("Save Config", function()
    local profileName = "Config_" .. os.date("%H%M%S")
    Window:SaveConfig(profileName)
    Window:Notify("Saved: " .. profileName)
end)

-- Load config
SaveLoadSection:CreateButton("Load Config", function()
    local profiles = Window:GetProfiles()
    
    if #profiles == 0 then
        Window:Notify("No saved profiles")
        return
    end
    
    print("=== Available Profiles ===")
    for i, name in ipairs(profiles) do
        print(i .. ". " .. name)
    end
    
    Window:Notify("Check console for profiles")
    Window:LoadConfig(profiles[1])
    Window:Notify("Loaded: " .. profiles[1])
end)

-- List profiles
SaveLoadSection:CreateButton("List Profiles", function()
    local profiles = Window:GetProfiles()
    
    if #profiles == 0 then
        Window:Notify("No saved profiles")
    else
        print("=== Saved Profiles ===")
        for i, name in ipairs(profiles) do
            print(i .. ". " .. name)
        end
        Window:Notify(#profiles .. " profiles (check console)")
    end
end)

SaveLoadSection:CreateDivider()

-- Export
SaveLoadSection:CreateButton("Export to Clipboard", function()
    Window:ExportConfig()
    Window:Notify("Config copied to clipboard!")
end)

-- Settings
local SettingsSection = ConfigPage:CreateSection("Settings")

local autoSave = SettingsSection:CreateToggle("Auto-Save", {
    Toggled = true
}, function(value)
    Window:SetAutoSave(value)
    Window:Notify("Auto-save: " .. (value and "ON" or "OFF"))
end)

SettingsSection:CreateLabel("Auto-save changes to current profile")


--â•”â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•—
--â•‘                           INITIALIZATION                                 â•‘
--â•‘                                                                          â•‘
--â•‘  This loads the default config and shows welcome message                â•‘
--â•šâ•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•

-- Load or create default profile
task.wait(1)
local profiles = Window:GetProfiles()
if table.find(profiles, "Default") then
    Window:LoadConfig("Default")
    print("[" .. SCRIPT_NAME .. "] Loaded default profile")
else
    Window:SaveConfig("Default")
    print("[" .. SCRIPT_NAME .. "] Created default profile")
end

-- Welcome notification
Window:Notify("Welcome to " .. SCRIPT_NAME .. "!")

-- Console output
print("â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•")
print("  " .. SCRIPT_NAME .. " " .. VERSION)
